{
"name": "Getting Started Example",
"version": "1.0",
"description": "Build an Extension",
"permissions": ["tabs", "activeTab", "declarativeContent" ,"storage"],
"background": {
  "scripts": ["background.js"],
  "persistent": false
},
"page_action": {
  "default_popup": "popup_original.html",
  "default_icon": {
    "16": "public/images/get_started16.png",
    "32": "public/images/get_started32.png",
    "48": "public/images/get_started48.png",
    "128": "public/images/get_started128.png"
  }
},
"icons": {
  "16": "public/images/get_started16.png",
  "32": "public/images/get_started32.png",
  "48": "public/images/get_started48.png",
  "128": "public/images/get_started128.png"
},
"options_page": "options.html",
"manifest_version": 2
}
